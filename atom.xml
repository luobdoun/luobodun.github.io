  <entry>
    <title>昨天阿泥跟朋友吃飯</title>
    <link href="http://luoboso.github.io/posts/06182023-b68ce8e5b1c2.html"/>
    <id>http://luoboso.github.io/posts/06182023-b68ce8e5b1c2.html</id>
    <published>2023-06-18T14:39:00.000Z</published>
